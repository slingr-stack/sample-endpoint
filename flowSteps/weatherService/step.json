{
  "label": "Weather service",
  "name": "weatherService",
  "category": "integrations",
  "description": "Returns current weather for a given city",
  "inputs": [
    {
      "label": "City",
      "name": "city",
      "type": "text",
      "description": "City name, if blank it will use a default city (NY)",
      "required": "true"
    }
      ],
  "outputs": [
    {
      "label": "Temperature",
      "name":  "temperature",
      "type": "decimal",
      "description": "Temperature"
    },
    {
      "label": "Pressure",
      "name":  "pressure",
      "type": "decimal",
      "description": "Pressure"
    },
    {
      "label": "Humidity",
      "name":  "humidity",
      "type": "decimal",
      "description": "Humidity"
    }
  ]
}
